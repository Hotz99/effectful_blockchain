    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^22.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "effect": "^3.17.7",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  },
  "pnpm": {
    "patchedDependencies": {}
  }
