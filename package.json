{
  "name": "effectful_blockchain",
  "version": "0.1.0",
  "packageManager": "bun@1.3.1",
  "license": "MIT",
  "description": "Cryptographic data structures for blockchain applications using Effect-TS",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && bun run copy-package-json",
    "build:ts": "tsup",
